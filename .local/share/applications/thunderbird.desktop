#!/usr/bin/env xdg-open

[Desktop Entry]
Version=1.0
Type=Application
Terminal=false
Exec=docker exec desktop bash -l -c "thunderbird %u"
Name=Thunderbird Mail
Comment=Send and receive mail with Thunderbird
GenericName=Mail Client
Icon=thunderbird
Categories=Network;Email;
MimeType=text/html;text/xml;application/xhtml+xml;application/rss+xml;x-scheme-handler/mailto;
StartupWMClass=Thunderbird
StartupNotify=true
